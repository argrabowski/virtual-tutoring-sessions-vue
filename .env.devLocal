## Development Environment in Local Mode

# Standard Information
VUE_APP_APPEND_TITLE = (DL)
VUE_APP_URL_SUB = -dev
VUE_APP_COOKIE_SUFFIX = Dev

# General Settings
VUE_APP_DEPLOYED = false
CYPRESS_ENV = devLocal

# VTS API
VUE_APP_VTS_URL = http://localhost:8081/VirtualTutoringSessions

# LoginPortal API
VUE_APP_LOGIN_PORTAL_URL = https://accounts${VUE_APP_URL_SUB}.assistments.org
