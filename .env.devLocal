# Core API
VUE_APP_ASSISTMENTS_URL = https://internal${VUE_APP_URL_SUB}.assistments.org
VUE_APP_ASSISTMENTS_API = ${VUE_APP_ASSISTMENTS_URL}/api

# Front End
VUE_APP_FRONT_PORT = 8080
VUE_APP_FRONT_URL = http://localhost:${VUE_APP_FRONT_PORT}
